		"@typescript-eslint/utils": "^8.50.0",
		"eslint": "^9.39.1",
		"eslint-plugin-no-server-imports": "workspace:*",
		"eslint-plugin-svelte": "^3.13.1",
		"pino": "^10.1.0",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1",
		"vite": "^7.3.0"
	}
